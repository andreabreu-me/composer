{
    "name": "cottontail-frontend",
    "version": "0.2.0",
    "license": "Apache-2.0",
    "homepage": "https://github.com/rabix/cottontail-frontend#readme",
    "angular-cli": {},
    "bugs": {
        "url": "https://github.com/rabix/cottontail-frontend/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/rabix/cottontail-frontend.git"
    },
    "scripts": {
        "build": "npm run compile:electron && npm run compile:angular && npm run package",
        "changelog": "./node_modules/.bin/git-changelog",
        "compile:angular": "./node_modules/.bin/ng build -prod -aot",
        "compile:electron": "./node_modules/.bin/tsc -p electron && cp -R electron/src/splash electron/dist/src",
        "compile:electron:watch": "./node_modules/.bin/tsc -p electron --watch",
        "lint": "./node_modules/.bin/ng lint",
        "package": "./scripts/electron-build.sh",
        "postinstall": "npm run update-webdriver && cd electron && npm install",
        "serve": "./node_modules/.bin/ng serve",
        "start:electron": "./node_modules/.bin/electron ./electron",
        "test": "npm run test:electron && npm run test:angular",
        "test:angular": "./node_modules/.bin/ng test --watch false",
        "test:angular:watch": "./node_modules/.bin/ng test",
        "test:e2e": "./node_modules/.bin/ng e2e",
        "test:electron": "./node_modules/electron/cli.js ./electron/node_modules/.bin/_mocha --recursive --ui bdd 'electron/dist/**/*.spec.js'",
        "travis": "npm run build && npm test",
        "update-webdriver": "node node_modules/protractor/bin/webdriver-manager update",
        "i18n": "ng-xi18n"
    },
    "private": true,
    "dependencies": {
        "@angular/common": "^4.0.0",
        "@angular/compiler": "^4.0.0",
        "@angular/core": "^4.0.0",
        "@angular/forms": "^4.0.0",
        "@angular/http": "^4.0.0",
        "@angular/platform-browser": "^4.0.0",
        "@angular/platform-browser-dynamic": "^4.0.0",
        "@angular/router": "^4.0.0",
        "ajv": "^4.11.6",
        "angular2-moment": "^1.2.0",
        "bootstrap": "4.0.0-alpha.5",
        "brace": "^0.10.0",
        "core-js": "^2.4.1",
        "cwl-svg": "^0.0.33",
        "cwlts": "^1.13.10",
        "font-awesome": "^4.7.0",
        "jailed": "^0.3.1",
        "jquery": "^3.2.0",
        "js-yaml": "^3.8.3",
        "markdown-it-lazy-headers": "^0.1.3",
        "ng-trim-value-accessor": "0.0.5",
        "ngx-pipes": "^1.6.1",
        "rxjs": "^5.1.0",
        "selectize": "^0.12.4",
        "zone.js": "0.8.5"
    },
    "devDependencies": {
        "@angular/cli": "^1.0.0",
        "@angular/compiler-cli": "^4.0.0",
        "@types/ace": "^0.0.32",
        "@types/jasmine": "2.5.47",
        "@types/jquery": "^2.0.41",
        "@types/js-yaml": "^3.5.29",
        "@types/markdown-it": "0.0.2",
        "@types/node": "^7.0.12",
        "@types/selectize": "^0.12.28",
        "codelyzer": "^3.0.1",
        "electron": "^1.6.6",
        "electron-packager": "^8.5.2",
        "git-changelog": "^1.1.2",
        "glob": "^7.1.1",
        "jasmine-core": "~2.5.2",
        "jasmine-spec-reporter": "~3.2.0",
        "karma": "^1.6.0",
        "karma-browserify": "^5.1.1",
        "karma-chrome-launcher": "~2.0.0",
        "karma-cli": "~1.0.1",
        "karma-coverage-istanbul-reporter": "^1.0.0",
        "karma-electron": "^5.1.1",
        "karma-jasmine": "~1.1.0",
        "karma-jasmine-html-reporter": "^0.2.2",
        "markdown-it": "^8.3.0",
        "protractor": "~5.1.0",
        "ts-node": "^3.0.2",
        "tslint": "^5.4.2",
        "typescript": "^2.2.1",
        "v8-profiler": "^5.7.0"
    },
    "engines": {
        "node": ">= 7.4.0",
        "npm": ">= 3"
    }
}
